def f(): Normal[Math] = {
  return 42
}

@main def main() = {
  // completion record
  call comp = clo<f>()
  assert (= comp.Type ~normal~)
  assert (= comp.Value 42)
  assert (= comp.Target ~empty~)

  // list
  let list = (new [2, 1]) [#1]
  assert (= list[0] 2)
  assert (= list[1] 1)

  // map is in map.ir

  // str
  let str = "abc"
  assert (= str[0] 97cu)
  assert (= str[0f] 97cu)
  assert (= str[1] 98cu)
  assert (= str[1f] 98cu)
  assert (= str[2] 99cu)
  assert (= str[2f] 99cu)
  assert (= str.length 3)
}
